<h6>Dashboard</h6>

<h3>Welcome <%= name %></h3>

<p>Here are your stories</p>

<% if(stories.length > 0){ %>
<table class="stripped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Date</th>
      <th>Status</th>
      <th></th>
    </tr>
    <tbody>
      <%stories.forEach(({_id, title, status, createdAt}) => { %>
        <tr>
          <td>
            <a href="/stories/<%=_id%>"><%=title%></a>
          </td>
          <td><%= formatDate(createdAt, 'MMMM Do YYYY, h:mm:ss a') %></td>
          <td><span class="dash-status">
            <%=status%>
          </span></td> 
        </tr>
      <%})%>
    </tbody>
  </thead>
</table>
<%}else{%>
<p>You have not created any stories</p>
<%}%>
